<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit trunk//EN" "http://www.gwtproject.org/doctype/2.8.0/gwt-module.dtd">
<module rename-to="html">
	<inherits name='com.badlogic.gdx.backends.gdx_backends_gwt' />

	<inherits name='Main' />
	<entry-point class='ru.neyvan.hm.client.HtmlLauncher' />
	<set-configuration-property name='xsiframe.failIfScriptTag' value='FALSE'/>
	<set-configuration-property name="gdx.assetpath" value="../android/assets" />
	<set-property name="user.agent" value="gecko1_8, safari"/>
	<collapse-property name="user.agent" values="*" />

	<extend-configuration-property name="gdx.reflect.include" value="ru.neyvan.hm.levels.Level" />
	<extend-configuration-property name="gdx.reflect.include" value="ru.neyvan.hm.levels.LevelLoader" />
	<extend-configuration-property name="gdx.reflect.include" value="ru.neyvan.hm.levels.Check" />
	<extend-configuration-property name="gdx.reflect.include" value="ru.neyvan.hm.game.Symbol" />
	<extend-configuration-property name="gdx.reflect.include" value="ru.neyvan.hm.game.GameData" />
	<extend-configuration-property name="gdx.reflect.include" value="ru.neyvan.hm.game.GameDataLoader" />
	<extend-configuration-property name="gdx.reflect.include" value="ru.neyvan.hm.terms" />
	<extend-configuration-property name="gdx.reflect.include" value="ru.neyvan.hm.surprises" />
	<extend-configuration-property name="gdx.reflect.include" value="ru.neyvan.hm.impacts" />
	<extend-configuration-property name="gdx.reflect.include" value="ru.neyvan.hm.states" />
	<extend-configuration-property name="gdx.reflect.include" value="ru.neyvan.hm.screens" />
	<extend-configuration-property name="gdx.reflect.include" value="com.badlogic.gdx.graphics.g2d.ParticleEffect" />
	<extend-configuration-property name="gdx.reflect.include" value="com.badlogic.gdx.scenes.scene2d.ui.Skin" />
	<extend-configuration-property name="gdx.reflect.include" value="com.badlogic.gdx.graphics.Texture" />
	<extend-configuration-property name="gdx.reflect.include" value="com.badlogic.gdx.graphics.g2d.TextureAtlas" />
</module>
